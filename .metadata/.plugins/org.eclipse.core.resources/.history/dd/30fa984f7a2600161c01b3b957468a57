

public class Bubble_sort {
	public static void main(String[] args){
	int x;
	int y;
	int i;
	int temp = 0;
	int [] winmine={1,4,7,6,8,9,0};
	for (i=0;i<winmine.length-1;i++)
	{
		for (x=0;x<winmine.length-1;x++){
			if (winmine[i]<winmine[i+1])
				temp=winmine[i];
			winmine[i]=winmine[i+1];
			winmine[i+1]=temp;
			
				
		}
	}
	for(y=0;y<winmine.length-1;y++){
		System.out.println(winmine[y]);
	}
	}
	

}
